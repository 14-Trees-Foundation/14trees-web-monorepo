{
  "name": "front-page",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev --port 3011",
    "clear-content": "rm -rf $PWD/content/*",
    "fetch-content": "dotenv -- yarn workspace notion-sync fetch --out $PWD/src/data/content --databases $PWD/src/assets/notion-databases.json",
    "copy-content": "cp -r $PWD/src/data/content/*/files $PWD/public/content/",
    "build": "next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@heroicons/react": "^2.0.16",
    "@notionhq/client": "^2.2.4",
    "framer-motion": "^11.0.3",
    "next": "^14.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.8",
    "ui": "*"
  },
  "devDependencies": {
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "@babel/core": "^7.0.0",
    "@types/node": "^17.0.12",
    "autoprefixer": "^10.4.13",
    "eslint": "7.32.0",
    "eslint-config-custom": "*",
    "postcss": "^8.4.21",
    "schema": "*",
    "tailwind-config": "*",
    "tailwindcss": "^3.2.7",
    "tsconfig": "*"
  }
}
