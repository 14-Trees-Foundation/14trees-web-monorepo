{
  "include": [
    "src/**/*.ts",
    "src/**/*.js"
  ],
  "exclude": [
    "src/**/*.d.ts",
    "src/swagger.ts",
    "src/testServer.ts", 
    "test/**/*",
    "coverage/**/*",
    "node_modules/**/*",
    "src/swagger_output.json",
    "**/*error*.js",
    "**/*Error*.js"
  ],
  "exclude-node-modules": true,
  "extension": [
    ".ts",
    ".js"
  ],
  "reporter": [
    "text",
    "html",
    "lcov"
  ],
  "report-dir": "coverage",
  "check-coverage": false,
  "lines": 85,
  "statements": 85,
  "functions": 85,
  "branches": 75,
  "instrument": true,
  "sourceMap": true,
  "all": true,
  "skip-full": true,
  "cache": true,
  "temp-dir": "./coverage/.nyc_output",
  "excludeAfterRemap": false,
  "produce-source-map": true
}